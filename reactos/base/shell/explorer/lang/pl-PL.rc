IDM_TRAYWND MENU
BEGIN
    POPUP ""
    BEGIN
        MENUITEM SEPARATOR
        MENUITEM "&Ustaw datę/godzinę", ID_SHELL_CMD_ADJUST_DAT
        MENUITEM "&Dostosuj powiadomienia...", ID_SHELL_CMD_CUST_NOTIF
        MENUITEM SEPARATOR
        MENUITEM "Okna &kaskadowo", ID_SHELL_CMD_CASCADE_WND
