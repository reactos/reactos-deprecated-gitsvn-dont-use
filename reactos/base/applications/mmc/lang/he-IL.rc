LANGUAGE LANG_HEBREW, SUBLANG_DEFAULT

IDM_CONSOLE_SMALL MENU
BEGIN
    POPUP "&קובץ"
    BEGIN
        MENUITEM "&חדש\tCtrl+N", IDM_FILE_NEW
        MENUITEM "&פתח...\tCtrl+O", IDM_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "&יציאה\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "&עזרה"
    BEGIN
        MENUITEM "&אודות ReactOS Management Console", IDM_HELP_ABOUT
    END
END

IDM_CONSOLE_LARGE MENU
BEGIN
    POPUP "&קובץ"
    BEGIN
        MENUITEM "&חדש\tCtrl+N", IDM_FILE_NEW
        MENUITEM "&פתח...\tCtrl+O", IDM_FILE_OPEN
        MENUITEM "&שמור", IDM_FILE_SAVE
        MENUITEM "שמור בש&ם...", IDM_FILE_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "&יציאה\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "&Windows"
    BEGIN
        MENUITEM "List", IDM_WINDOWS_LIST
    END
    POPUP "&עזרה"
    BEGIN
        MENUITEM "&אודות ReactOS Management Console", IDM_HELP_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_APPTITLE "ReactOS Management Console"
    IDS_CONSOLETITLE "Console%1!u!"
END
