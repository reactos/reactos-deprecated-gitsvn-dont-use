LANGUAGE LANG_MALAY, SUBLANG_DEFAULT

IDM_CONSOLE_SMALL MENU
BEGIN
    POPUP "&Fail"
    BEGIN
        MENUITEM "Baru(&N)\tCtrl+N", IDM_FILE_NEW
        MENUITEM "Buka(&O)...\tCtrl+O", IDM_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "Keluar(&X)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "Bantuan(&H)"
    BEGIN
        MENUITEM "Tent&ang Konsol Pengurusan ReactOS", IDM_HELP_ABOUT
    END
END

IDM_CONSOLE_LARGE MENU
BEGIN
    POPUP "&Fail"
    BEGIN
        MENUITEM "Baru(&N)\tCtrl+N", IDM_FILE_NEW
        MENUITEM "Buka(&O)...\tCtrl+O", IDM_FILE_OPEN
        MENUITEM "&Simpan", IDM_FILE_SAVE
        MENUITEM "Simp&an sebagai...", IDM_FILE_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "Keluar(&X)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "Tertingkap(&W)"
    BEGIN
        MENUITEM "Senarai", IDM_WINDOWS_LIST
    END
    POPUP "Bantuan(&H)"
    BEGIN
        MENUITEM "Tent&ang Konsol Pengurusan ReactOS", IDM_HELP_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_APPTITLE "Konsol Pengurusan ReactOS"
    IDS_CONSOLETITLE "Konsol%1!u!"
END
