    LTEXT "Non disponibile", IDD_ATTRIBUTESTEXT7, 60, 15, 90, 10
    LTEXT "Non disponibile", IDD_ATTRIBUTESTEXT8, 60, 25, 90, 10
    GROUPBOX "Unità di misura", IDD_ATTRIBUTESGROUP1, 6, 57, 158, 27
