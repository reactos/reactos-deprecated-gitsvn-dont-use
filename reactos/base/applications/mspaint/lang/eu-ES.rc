    EDITTEXT IDD_STRETCHSKEWEDITVSKEW, 90, 118, 32, 12
    LTEXT "degrees", IDD_STRETCHSKEWTEXTVDEG, 125, 119, 30, 10
    DEFPUSHBUTTON "Ados", IDOK, 170, 8, 48, 14
    PUSHBUTTON "Utzi", IDCANCEL, 170, 24, 48, 14
END

