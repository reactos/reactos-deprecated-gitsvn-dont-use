LANGUAGE LANG_UKRAINIAN, SUBLANG_DEFAULT

IDC_MAGNIFIER MENU
BEGIN
    POPUP "&Файл"
    BEGIN
        MENUITEM "В&ихід", IDM_EXIT
        MENUITEM "П&араметри", IDM_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&Про програму", IDM_ABOUT
    END
END

IDC_MAGNIFIER ACCELERATORS
BEGIN
    "?", IDM_ABOUT, ASCII, ALT
    "/", IDM_ABOUT, ASCII, ALT
END

