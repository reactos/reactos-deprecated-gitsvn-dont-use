    POPUP "&Bantuan"
    BEGIN
        MENUITEM "Topik &Bantuan\tF1", ID_HELP_HELPTOPICS
        MENUITEM SEPARATOR
        MENUITEM "&Tentang Editor Registri", ID_HELP_ABOUT
    END
END

