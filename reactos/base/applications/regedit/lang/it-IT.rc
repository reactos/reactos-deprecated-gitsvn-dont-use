        MENUITEM "&Taglia", ID_HEXEDIT_CUT
        MENUITEM "&Copia", ID_HEXEDIT_COPY
        MENUITEM "&Incolla", ID_HEXEDIT_PASTE
        MENUITEM "&Cancella", ID_HEXEDIT_DELETE
        MENUITEM SEPARATOR
        MENUITEM "&Seleziona tutto", ID_HEXEDIT_SELECT_ALL
    END
