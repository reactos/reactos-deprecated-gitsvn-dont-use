BEGIN
    IDS_STRPOS "Pozícia: %.2f s"
    IDS_STRDUR "Dĺžka: %.2f s"
    IDS_STRBUF "Buffer: %.2f kb"
    IDS_STRFMT "%.1f kHz %u bitov"
    IDS_STRMONO "mono"
    IDS_STRSTEREO "stereo"
    IDS_STRCHAN "%s"
END
