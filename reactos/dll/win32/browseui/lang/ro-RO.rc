    COMBOBOX IDC_ICONOPTIONS, 52, 18, 123, 57, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
END

/* Accelerators */

IDR_ACCELERATORS ACCELERATORS
BEGIN
